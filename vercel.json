{
	"version": 2,
	"github": {
		"silent": true
	},
	"builds": [
		{
			"src": "index.js",
			"use": "@now/node-server",
			"config": {
				"maxLambdaSize": "15mb"
			}
		}
	],
	"routes": [
		{
			"src": "/(.*)",
			"dest": "index.js",
			"methods": ["GET"],
			"headers": {
				"Access-Control-Allow-Origin": "*"
			}
		}
	]
}
